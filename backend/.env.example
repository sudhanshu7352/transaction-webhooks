# Put your actual cloud MySQL URI here, example:
# mysql+pymysql://user:password@host:3306/dbname
DATABASE_URL=mysql+pymysql://DB_USER:DB_PASS@DB_HOST:3306/DB_NAME

# Redis (for local development with docker-compose)
REDIS_URL=redis://redis:6379/0

# Celery broker and backend
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/1

# Optional: set LOG_LEVEL
LOG_LEVEL=info
